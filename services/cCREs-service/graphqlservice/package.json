{
    "name": "ccre-service",
    "version": "3.10.3",
    "description": "Service that stores and retrieves information about cCREs and rDHSs",
    "main": "index.js",
    "repository": "git@github.com:/weng-lab/ccre-service",
    "author": "",
    "license": "MIT",
    "private": true,
    "jest": {
        "globals": {
            "ts-jest": {
                "tsconfig": "tsconfig.json"
            }
        },
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testMatch": ["**/test/**/*.test.(ts|js)"],
        "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
        "testEnvironment": "node",
        "testTimeout": 30000
    },
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "test": ". ./scripts/setup-environment.sh && jest --detectOpenHandles",
        "debug": ". ./scripts/setup-environment.sh && node --inspect dist/server.js",
        "prettier": "prettier --write ./src/**/*.ts ./test/**/*.ts",
        "precommit": "yarn prettier"
    },
    "dependencies": {
        "@apollo/federation": "^0.20.7",
        "@tensorflow/tfjs": "^3.4.0",
        "@tensorflow/tfjs-backend-cpu": "^3.4.0",
        "@tensorflow/tfjs-core": "^3.21.0",
        "@tensorflow/tfjs-node": "^3.4.0",
        "@types/request": "^2.48.5",
        "apollo-graphql": "^0.6.0",
        "apollo-server-express": "^2.1.0",
        "bigwig-reader": "^1.3.1",
        "cors": "^2.8.5",
        "dataloader": "^1.4.0",
        "express": "^4.16.4",
        "graphql": "^14.3.1",
        "pg-native": "^3.0.0",
        "pg-promise": "^10.8.1",
        "queryz": "^0.0.3",
        "request": "^2.88.2",
        "seedrandom": "^3.0.5",
        "sync-request": "^6.1.0"
    },
    "devDependencies": {
        "@types/express": "^4.16.0",
        "@types/jest": "^23.3.5",
        "@types/mocha": "^10.0.0",
        "@types/node": "^10.12.0",
        "@types/supertest": "^2.0.7",
        "jest": "^24.9.0",
        "prettier": "^1.15.0",
        "supertest": "^3.4.2",
        "ts-jest": "^24.3.0",
        "typescript": "^4.1.3"
    },
    "prettier": {
        "printWidth": 160,
        "tabWidth": 4,
        "arrowParens": "always",
        "parser": "typescript"
    }
}
